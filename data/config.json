{
  "mesh": {
    "nx": 20,
    "ny": 20,
    "nz": 20,
    "dx": 0.05,
    "dy": 0.05,
    "dz": 0.05
  },

  "time": {
    "dt": 0.001,
    "t_end": 0.05
  },

  "physics": {
    "nu": 1e-5,
    "k": "1e10 "
  },

  "forces": {
    "fx": "cos(t) * sin(x) * sin(y) * sin(z)     - 3 * sin(t) * sin(x) * sin(y) * cos(z) ",
    "fy": "cos(t) * cos(x) * cos(y) * cos(z)      - 3 * sin(t) * cos(x) * cos(y) * sin(z) ",
    "fz": "cos(t) * cos(x) * sin(y) * (sin(z) + cos(z))  - 3 * cos(x) * sin(y) * (cos(z) + sin(z))"
  },

 "initial_conditions": {
  "u": " sin(t) * sin(x) * sin(y) * sin(z)",
  "v": " sin(t) * cos(x) * cos(y) * cos(z)",
  "w": " sin(t) * cos(x) * sin(y) * (sin(z) + cos(z))",
  "p": " sin(t) * 3.0 * cos(x) * sin(y) * (sin(z) - cos(z))"
},

  "boundary_conditions": {
    "u": "0",
    "v": "0",
    "w": "0"
  },

  "output": {
    "enabled": true,
    "dir": "./output",
    "base_filename": "simulation_output",
    "frequency": 1
  },

  "logging": {
    "verbose": true,
    "log_to_file": false,
    "log_to_console": true,
    "filename": "simulation.log",
    "frequency": 20
  }
}
