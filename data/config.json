},

  "boundary_conditions": {
    "u": " sin(t) * sin(x) * sin(y) * sin(z)",
    "v": " sin(t) * cos(x) * cos(y) * cos(z)",
    "w": " sin(t) * cos(x) * sin(y) * (sin(z) + cos(z))"
  },

  "output": {
