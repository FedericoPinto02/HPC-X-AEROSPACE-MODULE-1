  "forces": {
    "fx": "cos(t) * sin(x) * sin(y) * sin(z) + sin(t) * ( (21 + 6e-10) * sin(x) * sin(y) * sin(z) - 3 * sin(x) * sin(y) * cos(z) )",
    "fy": "cos(t) * cos(x) * cos(y) * cos(z) + sin(t) * ( (21 + 6e-10) * cos(x) * cos(y) * cos(z) - 3 * cos(x) * cos(y) * sin(z) )",
    "fz": "cos(t) * cos(x) * sin(y) * (sin(z) + cos(z)) + sin(t) * ( (15 + 6e-10) * cos(x) * sin(y) * (sin(z) + cos(z)) )"
  },

 "initial_conditions": {
  "u": " sin(t) * sin(x) * sin(y) * sin(z)",
  "v": " sin(t) * cos(x) * cos(y) * cos(z)",
  "w": " sin(t) * cos(x) * sin(y) * (sin(z) + cos(z))",
