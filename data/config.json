{
  "mesh": {
    "nx": 10,
    "ny": 10,
    "nz": 10,
    "dx": 0.1,
    "dy": 0.1,
    "dz": 0.1
  },

  "time": {
    "dt": 0.01,
    "t_end": 1.0
  },

  "physics": {
    "nu": 1e-5,
    "k": "sin(x)*sin(y)*sin(z) + 10.0"
  },

  "forces": {
    "fx": "0.0 * cos(pi*z)",
    "fy": "0.0 * cos(pi*y)",
    "fz": "0.0 * cos(pi*x)"
  },

 "initial_conditions": {
  "u": "20 * sin(pi*x)^2 * sin(2*pi*y) * sin(2*pi*z)",
  "v": "-20 * sin(2*pi*x) * sin(pi*y)^2 * sin(2*pi*z)",
  "w": "0.0",
  "p": "1e5"
},

  "boundary_conditions": {
    "u": "0",
    "v": "0",
    "w": "0"
  },

  "output": {
    "enabled": true,
    "dir": "./output",
    "base_filename": "simulation_output",
    "frequency": 1
  },

  "logging": {
    "verbose": true,
    "log_to_file": false,
    "log_to_console": true,
    "filename": "simulation.log",
    "frequency": 20
  }
}
